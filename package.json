{
  "name": "utran",
  "version": "1.0.0",
  "description": "Lightweight cross-language rpc, but not just rpc, lets you utran everything easily.",
  "main": "./lib/utran.min.js",
  "module": "index.js",
  "types": "./types/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "types/",
    "dist/",
    "licenses/",
    "NOTICE"
  ],
  "keywords": [
    "rpc",
    "websocket",
    "cross-language",
    "python",
    "h5",
    "nodejs",
    "typescript"
  ],
  "author": "utran",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint src --fix",
    "clean": "node scripts/clean.js",
    "build-core:dev": "cross-env NODE_ENV=development node scripts/build.js",
    "build-core:prod": "cross-env NODE_ENV=production node scripts/build.js",
    "build-core": "npm run build-core:dev && npm run build-core:prod",
    "build-h5:dev": "cross-env NODE_ENV=development node scripts/build.js --ish5=true",
    "build-h5:prod": "cross-env NODE_ENV=production node scripts/build.js --ish5=true",
    "build-h5": "npm run build-h5:dev && npm run build-h5:prod",
    "build-dts": "dts-bundle-generator --no-banner true --umd-module-name utran -o types/index.d.ts src/index.ts",
    "build": "npm run lint && npm run clean && npm run build-core && npm run build-dts && npm run build-h5",
    "test": "node tests/test.js",
    "testtemp": "tsc tests/temp.ts && node tests/temp.js"
  },
  "dependencies": {
    "@types/ws": "^8.5.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-eslint": "^9.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^7.0.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "rollup": "^3.2.5",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-progress": "^1.1.2",
    "typescript": "^5.0.4",
    "yargs": "^17.7.1"
  }
}
